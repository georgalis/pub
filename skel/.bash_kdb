        expr "$bg" : '^[[:xdigit:]]\{8\}$' > /dev/null || bg="$(k4_headg "^%km " "$uidhln/$uid" | tr '_' ' ' | awk 'NR==1 {print $3}')";
        bg="$(printf "%06x\n" "$(( 0x${bg} % 0xffffff ))")";
        fg="bg xor nulltime xor color";
